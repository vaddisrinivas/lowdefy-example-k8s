kind: Deployment
apiVersion: apps/v1
metadata:
  name: test
  labels:
    run: test
spec:
  replicas: 1
  selector:
    matchLabels:
      run: test
  template:
    metadata:
      creationTimestamp: null
      labels:
        run: test
    spec:
      containers:
        - name: test
          image: 'imvegeta/websheets:lowdefy'
          env:
            - name: LOWDEFY_SERVER_PORT
              value: '3000'
          resources: {}
---
kind: Service
apiVersion: v1
metadata:
  name: test
  labels:
    app: test
spec:
  ports:
    - name: 5678-443
      protocol: TCP
      port: 5678
      targetPort: 3000
      nodePort: 30864
  selector:
    app: test
  type: LoadBalancer
---
